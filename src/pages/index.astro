---
import { Image } from "astro:assets";
import Layout from "../layouts/Layout.astro";
import phone from "../icons/icons8-phone-50.png";
import email from "../icons/email.svg";
import linkedin from "../icons/icons8-linkedin-50.png";
import github from "../icons/icons8-github-24.png";
import javascript from "../icons/icons8-javascript-48.png";
import golang from "../icons/icons8-golang-48.png";
import php from "../icons/icons8-php-48.png";
import python from "../icons/icons8-python-48.png";
import react from "../icons/icons8-react-a-javascript-library-for-building-user-interfaces-48.png";
import html from "../icons/icons8-html-5-is-a-software-solution-stack-that-defines-the-properties-and-behaviors-of-web-page-48.png";
import css from "../icons/icons8-css-48.png";
import nodejs from "../icons/icons8-nodejs-48.png";
import express from "../icons/icons8-express-js-50.png";
import mongo from "../icons/icons8-mongodb-48.png";
import sql from "../icons/icons8-sql-48.png";
import slack from "../icons/icons8-slack-48.png";
import bot from "../icons/icons8-bot-48.png";
import websocket from "../icons/websocket.svg";
---

<Layout title="Mukul Mahajan Singraur">
	<main>
		<header>
			<svg
				class="background"
				viewBox="80 70 200 200"
				xmlns="http://www.w3.org/2000/svg"
				style="width: 20rem; top: 0; overflow: visible"
			>
				<path
					fill="#F1C21B80"
					d="M45.6,27.8C33.8,46.7,-18.4,43.8,-32.8,23.4C-47.1,3,-23.6,-34.9,2.6,-33.4C28.7,-31.9,57.3,8.9,45.6,27.8Z"
					transform="translate(100 100)"></path>
			</svg>
			<h1 id="name">Mukul Mahajan Singraur</h1>
			<section id="contacts">
				<div>
					<Image
						alt="phone"
						src={phone}
						height={18}
						width={18}
						style={{ objectFit: "contain" }}
					/>
					<a href="tel:+919161061356"> +91 9161 06 1356</a>
				</div>
				<div>
					<Image
						alt="email"
						src={email}
						height={12}
						width={18}
						style={{ objectFit: "contain" }}
					/>
					<a href="mailto:rammmukul@outlook.com">
						rammmukul@outlook.com
					</a>
				</div>
				<div>
					<Image
						alt="github"
						src={github}
						height={15}
						width={18}
						style={{ objectFit: "contain" }}
					/>
					<a href="https://github.com/rammmukul"> rammmukul</a>
				</div>
				<div>
					<Image
						alt="linkedin"
						src={linkedin}
						height={18}
						width={18}
						style={{ objectFit: "contain" }}
					/>
					<a
						href="https://www.linkedin.com/in/mukul-mahajan-singraur"
					>
						mukul-mahajan-singraur
					</a>
				</div>
			</section>
		</header>
		<hr />
		<section id="technical-skills">
			<h2>Technical Skills</h2>
			<h3>Programming languages</h3>
			<span class="pill">
				<Image alt="" src={python} height={15} /> python
			</span>
			<span class="pill">
				<Image alt="" src={javascript} height={15} /> JavaScript
			</span>
			<span class="pill">
				<Image alt="" src={golang} height={15} /> go
			</span>
			<span class="pill">
				<Image alt="" src={php} height={15} /> PHP
			</span>
			<br />
			<h3>Front-end Development</h3>
			<span class="pill">
				<Image alt="" src={react} height={15} /> React.js
			</span>
			<span class="pill">
				<Image alt="" src={html} height={15} /> HTML 5
			</span>
			<span class="pill">
				<Image alt="" src={css} height={15} /> CSS 3
			</span>
			<span class="pill">
				<Image alt="" src={react} height={15} /> React Native
			</span>
			<br />
			<h3>Back-end Development</h3>
			<span class="pill">
				<Image alt="" src={mongo} height={15} /> MongoDB
			</span>
			<span class="pill">
				<Image alt="" src={sql} height={15} /> SQL
			</span>
			<span class="pill">
				<Image alt="" src={nodejs} height={15} /> node.js
			</span>
			<span class="pill">
				<Image alt="" src={express} height={15} /> express.js
			</span>
			<span class="pill">
				<Image alt="" src={golang} height={15} /> go
			</span>
		</section>
		<hr />
		<section style="position: relative;">
			<h2>Exotel (SDE 2) - Feb-2020 to Feb-2023</h2>
			<svg
				class="background"
				style="width: 40rem; left: -20rem; top: 0rem"
				viewBox="0 0 200 200"
				xmlns="http://www.w3.org/2000/svg"
			>
				<path
					fill="#FA4D5620"
					d="M43.9,-76C57.6,-68.2,69.7,-57.7,73.6,-44.6C77.5,-31.5,73.2,-15.7,72,-0.7C70.7,14.3,72.5,28.6,66.9,38.8C61.3,48.9,48.3,55,35.9,59.6C23.5,64.1,11.8,67.2,-1.9,70.4C-15.5,73.7,-31.1,77.2,-41.2,71.3C-51.3,65.5,-56.1,50.3,-56.7,36.9C-57.4,23.4,-53.9,11.7,-53.4,0.3C-52.8,-11,-55,-22.1,-52.4,-32.1C-49.8,-42.2,-42.3,-51.3,-32.8,-61.4C-23.2,-71.6,-11.6,-82.9,1.8,-85.9C15.2,-89,30.3,-83.9,43.9,-76Z"
					transform="translate(100 100)"></path>
			</svg>
			<h3 id="LCD" class="project">Live Call Dashboard</h3>
			<span class="pill centered">
				<span class="sr-only">tools used</span>
				<span class="tech-tag">
					<Image alt="" src={react} height={15} />
					<span class="sr-only">react</span>
				</span>
				<span class="tech-tag">
					<Image alt="" src={golang} height={15} />
					<span class="sr-only">golang</span>
				</span>
				<span class="tech-tag">
					<Image alt="" src={mongo} height={15} />
					<span class="sr-only">mongo</span>
				</span>
				<span class="tech-tag">
					<span>AWS</span>
				</span>
			</span>
			<p>
				<a
					href="https://support.exotel.com/support/solutions/articles/3000104288-call-centre-dashboard-private-beta-updated-"
				>
					A React based dashboard for clients to see the live
					analytics in a glance.
				</a>
			</p>
			<ul>
				<li>Ingested data from various micro-services</li>
				<li>Processed and saved data in Mongo, making it easy to query by time and entities (agents, groups)</li>
				<li>Aggregated data in 5 min chunks for historical stats</li>
				<li>
					Built charts to show most important live information and
					tables/cards for other information.
				</li>
				<li>Built REST APIs in go to serve analytics from MongoDB</li>
				<li class="impact">
					Account admins got a better idea of what is happening in
					their contact center at a glance.
				</li>
				<li class="impact">
					They could get granular and categorized data to assess their agent's and team's
					performance by following cross links which preserved applied
					date, group and agent filters.
				</li>
			</ul>
			<h3 class="project">Reports downloads</h3>
			<span class="pill centered">
				<span class="sr-only">tools used</span>
				<span class="tech-tag">
					<Image alt="" src={php} height={15} />
					<span class="sr-only">PHP</span>
				</span>
				<span class="tech-tag">
					<Image alt="" src={javascript} height={15} />
					<span class="sr-only">javascript</span>
				</span>
			</span>
			<p>Built daily and on demand report generation</p>
			<ul>
				<li>
					Build report pipeline to generate SMS and calls reports
				</li>
				<li>Added Filters to customize report to user's need</li>
				<li>Scheduled reports to be generated daily before working hours according to client's business hours</li>
				<li>Added fail safes so that report generation can resume after unexpected downtimes of system</li>
				<li>Compartmentalized data so that error in one clients data does not effect report for others</li>
				<li>Build UI for on demand report generation</li>
			</ul>
			<h2>Stanza Living (SDE 1) - Oct-2018 Feb-2020</h2>
			<h3 class="project">Public landing page and Internal dashboard</h3>
			<span class="pill centered">
				<span class="sr-only">tools used</span>
				<span class="tech-tag">
					<Image alt="" src={react} height={15} />
					<span class="sr-only">React Native</span>
				</span>
			</span>
			<ul>
				<li>Added analytics to the landing page of stanza living</li>
				<li class="impact">
					Management got better data on the interest of the potential
					customers
				</li>
				<li class="impact">
					PMs got better understanding for redesigning the pages
				</li>
			</ul>
			<h2>GeekSkool (bootcamp) - Feb-2018 to oct-2018</h2>
			<ul>
				<a href="https://github.com/rammmukul/elasticGuttenburgs"
					><li>
						Ingested Gutenberg data and made it fuzzy searchable.
						<span class="pill centered">
							<span class="sr-only">tools used</span>
							<span class="tech-tag">
								<Image alt="" src={javascript} height={15} />
								<span class="sr-only">javascript</span>
							</span>
							<span class="tech-tag">
								<Image alt="" src={nodejs} height={15} />
								<span class="sr-only">node</span>
							</span>
							<span class="tech-tag">
								<span>Elastic search</span>
							</span>
						</span>
					</li></a
				>
			<a id="last" href="https://github.com/rammmukul?tab=repositories"
				>more...</a
			>
			<svg
				class="background"
				viewBox="-50 -50 200 200"
				xmlns="http://www.w3.org/2000/svg"
				style="bottom: -100px; width: 30rem; right: -50px;"
			>
				<path
					fill="#FA4D5680"
					d="M22.1,15.3C11,31.9,-27.6,35.1,-35.9,20.1C-44.2,5.1,-22.1,-28.1,-2.8,-29.7C16.6,-31.3,33.2,-1.3,22.1,15.3Z"
					transform="translate(100 100)"></path>
			</svg>
		</section>
	</main>
</Layout>

<style>
	.sr-only {
		position: absolute;
		width: 1px;
		height: 1px;
		padding: 0;
		margin: -1px;
		overflow: hidden;
		clip: rect(0, 0, 0, 0);
		white-space: nowrap;
		border-width: 0;
	}
	a:link {
		color: rgb(1, 92, 2);
		text-decoration: underline;
		text-decoration-thickness: 0.5px;
		text-decoration-style: solid;
	}
	#contacts {
		display: flex;
		flex-direction: column;
		justify-content: center;
		height: -webkit-fill-available;
	}
	header {
		columns: 2;
	}
	#name {
		text-align: left;
		margin: unset;
		width: 5em;
	}
	.pill {
		display: inline-block;
		border: 2px solid rgb(117, 64, 64);
		border-radius: 999px;
		padding: 0 0.4em 0.1em 0.4em;
		margin: 0.1em 0.1em 0.1em 0;
		background-color: var(--light-background);
		font-size: 1.2rem;
	}
	.pill img {
		transform: translatey(2px);
	}
	.centered {
		padding: 0.1em 0.4em;
	}
	.project {
		display: inline-block;
	}
	.tech-tag {
		display: inline-block;
	}
	main {
		margin: auto;
		padding: 1rem;
		width: 800px;
		max-width: calc(100% - 2rem);
		font-size: 1.2rem;
		line-height: 1.5;
	}
	h1 {
		font-size: 3rem;
		font-weight: 700;
		line-height: 1;
		text-align: center;
		margin-bottom: 1em;
	}
	h2 {
		font-size: 2.4rem;
		margin: 0.1em 0;
	}
	h3 {
		font-size: 1.8rem;
		margin: 0.3em 0.5em 0.3em 0;
	}
	p {
		margin: 0.4em 0;
		font-size: 1.2rem;
		text-align: justify;
		text-indent: 1em;
		line-height: .3;
	}
	ul {
		margin: 0.2em 0;
	}
	#technical-skills h3 {
		display: inline;
	}
	.background {
		position: absolute;
		z-index: -1;
	}
	.impact::marker {
		content: "➤ ";
	}
</style>
